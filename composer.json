{
    "name": "dnadesign/silverstripe-elemental-userforms",
    "description": "Adds a new element for usersforms",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "element",
        "elemental",
        "content blocks",
        "userforms"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "John Milmine",
            "email": "john.milmine@dna.co.nz"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4 || ^8.0",
        "silverstripe/framework": "^4.10",
        "dnadesign/silverstripe-elemental": "^4.0",
        "silverstripe/userforms": "^5.0",
        "silverstripe/vendor-plugin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "DNADesign\\ElementalUserForms\\": "src/",
            "DNADesign\\ElementalUserForms\\Tests\\": "tests/php/",
            "DNADesign\\ElementalUserForms\\Tests\\Behat\\Context\\": "tests/behat/src"
        }
    }
}
